<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Loan">
<listeners>
<listener class-name="com.loan.terstcases.Listener"></listener>
</listeners>

  <test name="Car Loan">
  
    <classes>
      <class name="com.loan.terstcases.CarLoan"/>
      <class name="com.loan.terstcases.EducationLoan"/>
      <class name="com.loan.terstcases.HomeLoan"/>
    </classes>
    </test>
    <parameter name="URL" value="Google.com"></parameter>
    <test name="Personal Loan">
    <classes>
      <class name="com.loan.terstcases.PropertyLoan"/>
      <class name="com.loan.terstcases.PersonalLoan"/>
        </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->